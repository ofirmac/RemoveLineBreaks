{
    "manifest_version": 3,
    "name": "Remove Line Breaks",
    "version": "1.0",
    "permissions": ["clipboardRead", "clipboardWrite"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
  